<?xml version="1.0"?>
<Container version="2">
  <Name>jd</Name>
  <Repository>nevinee/jd:v4-bot</Repository>
  <Registry>https://hub.docker.com/r/nevinee/jd</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/nevinee/jd</Support>
  <Project>https://hub.docker.com/r/nevinee/jd</Project>
  <Overview>JD bot版</Overview>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:9876/</WebUI>
  <ExtraParams>-it</ExtraParams>
  <TemplateURL>https://raw.githubusercontent.com/sofs2005/unraidtemp/main/nev4.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/Orz-3/mini/master/Color/jd.png</Icon>
  <Description>JD bot版</Description>
  <Config Name="port" Target="5700" Default="9876" Mode="tcp" Description="web" Type="Port" Display="always" Required="false" Mask="false">5801</Config>
  <Config Name="config" Target="/jd/config" Default="/mnt/disk1/dockerfiles/jd/jd1/config/" Mode="rw" Description="config" Type="Path" Display="always" Required="true" Mask="false">/mnt/disk1/dockerfiles/jd/ql1/config/</Config>
  <Config Name="log" Target="/jd/log" Default="/mnt/disk1/dockerfiles/jd/jd1/log/" Mode="rw" Description="log" Type="Path" Display="always" Required="true" Mask="false">/mnt/disk1/dockerfiles/jd/ql1/log/</Config>
  <Config Name="own" Target="/jd/own" Default="/mnt/disk1/dockerfiles/jd/jd1/own/" Mode="rw" Description="db" Type="Path" Display="always" Required="true" Mask="false">/mnt/disk1/dockerfiles/jd/ql1/own/</Config>
  <Config Name="scripts" Target="/jd/scripts" Default="/mnt/disk1/dockerfiles/jd/jd1/scripts/" Mode="rw" Description="scripts" Type="Path" Display="always" Required="true" Mask="false">/mnt/disk1/dockerfiles/jd/jd1/scripts/</Config>
  <Config Name="ENABLE_HANGUP" Target="ENABLE_HANGUP" Default="true" Mode="" Description="crazy joy" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="ENABLE_TTYD" Target="ENABLE_TTYD" Default="true" Mode="" Description="autoweb" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="ENABLE_TG_BOT" Target="ENABLE_TG_BOT" Default="true" Mode="" Description="tgbot" Type="Variable" Display="always" Required="false" Mask="false">false</Config>
  <Config Name="ENABLE_WEB_PANEL" Target="ENABLE_WEB_PANEL" Default="true" Mode="" Description="web panel" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
</Container>
