<?xml version="1.0"?>
<Container version="2">
  <Name>jdup</Name>
  <Repository>registry-intl.cn-shenzhen.aliyuncs.com/soulmate_shell/soulmate:bot</Repository>
  <Registry>https://hub.docker.com/r/nevinee/jd</Registry>
  <Network>br0</Network>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/nevinee/jd</Support>
  <Project>https://hub.docker.com/r/nevinee/jd</Project>
  <Overview>JD bot改</Overview>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:[PORT:5678]/</WebUI>
  <ExtraParams>-it</ExtraParams>
  <TemplateURL>https://raw.githubusercontent.com/sofs2005/unraidtemp/main/v4up.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/Orz-3/mini/master/Color/jdczf.png</Icon>
  <Description>JD bot改</Description>
  <Config Name="port" Target="5678" Default="5678" Mode="tcp" Description="web" Type="Port" Display="always" Required="false" Mask="false">5678</Config>
  <Config Name="config" Target="/jd/config" Default="/mnt/dockerfiles/jd/jdg/config/" Mode="rw" Description="config" Type="Path" Display="always" Required="true" Mask="false">/mnt/dockerfiles/jd/jdg/config/</Config>
  <Config Name="log" Target="/jd/log" Default="/mnt/dockerfiles/jd/jdg/log/" Mode="rw" Description="log" Type="Path" Display="always" Required="true" Mask="false">/mnt/dockerfiles/jd/jdg/log/</Config>
  <Config Name="own" Target="/jd/own" Default="/mnt/dockerfiles/jd/jdg/own/" Mode="rw" Description="own" Type="Path" Display="always" Required="true" Mask="false">/mnt/dockerfiles/jd/jdg/own/</Config>
  <Config Name="botdiy" Target="/jd/jbot/diy" Default="/mnt/dockerfiles/jd/jdg/jbot/diy/" Mode="rw" Description="botdiy" Type="Path" Display="always" Required="true" Mask="false">/mnt/dockerfiles/jd/jdg/jbot/diy</Config>
  <Config Name="scripts" Target="/jd/scripts" Default="/mnt/dockerfiles/jd/jdg/scripts/" Mode="rw" Description="scripts" Type="Path" Display="always" Required="true" Mask="false">/mnt/dockerfiles/jd/jdg/scripts/</Config>
  <Config Name="ENABLE_HANGUP" Target="ENABLE_HANGUP" Default="true" Mode="" Description="crazy joy" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="ENABLE_TG_BOT" Target="ENABLE_TG_BOT" Default="true" Mode="" Description="tgbot" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="ENABLE_JF_BOT" Target="ENABLE_JF_BOT" Default="true" Mode="" Description="JF_BOT" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="ENABLE_TTYD" Target="ENABLE_TTYD" Default="true" Mode="" Description="TTYD" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="ENABLE_WEB_PANEL" Target="ENABLE_WEB_PANEL" Default="true" Mode="" Description="Panel" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
</Container>
