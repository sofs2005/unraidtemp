<?xml version="1.0"?>
<Container version="2">
  <Name>dailycheckin</Name>
  <Repository>sitoi/dailycheckin</Repository>
  <Registry>https://hub.docker.com/r/sitoi/dailycheckin</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://sitoi.github.io/dailycheckin/synology/</Support>
  <Project>https://github.com/Sitoi/dailycheckin</Project>
  <Overview>日常签到</Overview>
  <Category>Tools:</Category>
  <ExtraParams>-d</ExtraParams>
  <TemplateURL>https://raw.githubusercontent.com/sofs2005/unraidtemp/main/dailycheckin.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/Orz-3/mini/master/Color/time.png</Icon>
  <Description>日常签到</Description>
  <Config Name="config" Target="/dailycheckin/config" Default="/mnt/user/dockerfiles/dailycheckin/config" Mode="rw" Description="config" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/dockerfiles/dailycheckin/config</Config>
  <Config Name="log" Target="/dailycheckin/logs" Default="/mnt/user/dockerfiles/dailycheckin/logs" Mode="rw" Description="log" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/dockerfiles/dailycheckin/logs</Config>
</Container>
